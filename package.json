{
  "name": "protein-expression-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "large-start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
    "large-start-local": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --configuration=dev.local",
    "large-start-docker": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --configuration=dev.docker",
    "large-start-remote": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --configuration=dev.remote",
    "large-start-prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --configuration=production",
    "build": "ng build",
    "large-build": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build",
    "large-build-prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --prod",
    "test": "ng test --watch false",
    "test:watch": "ng test",
    "lint": "ng lint --format stylish",
    "e2e": "ng e2e --protractorConfig=e2e/protractor.conf.ts",
    "e2e-local": "ng e2e --protractorConfig=e2e/protractor.conf.ts --configuration=dev.local",
    "e2e-docker": "ng e2e --protractorConfig=e2e/protractor.conf.ts --configuration=dev.docker",
    "e2e-remote": "ng e2e --protractorConfig=e2e/protractor.conf.ts --configuration=dev.remote",
    "e2e-prod": "ng e2e --protractorConfig=e2e/protractor.conf.ts --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "22.1.1",
    "@ag-grid-enterprise/all-modules": "22.1.1",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngu/carousel": "^1.5.5",
    "angular-in-memory-web-api": "^0.9.0",
    "file-saver": "^2.0.2",
    "ngx-filesaver": "^8.1.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~12.12.17",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.5.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.5.4",
    "typescript": "~3.5.3"
  }
}
